source 'https://mirrors.tuna.tsinghua.edu.cn/git/CocoaPods/Specs.git'


platform :ios,  '8.0'
install! 'cocoapods',
:disable_input_output_paths => true

target 'FUP2A' do
  pod 'SVProgressHUD', '~> 2.0.3'
  pod 'AFNetworking', '~> 3.2.1', :subspecs => ['Reachability', 'Serialization', 'Security', 'NSURLSession']
  pod 'SDWebImage'
  pod 'Masonry',  '1.1.0'
end
